#include<bits/stdc++.h>
using namespace std;

int main()
{
    int a,b;
    int i=1;
    while(1)
    {
        cin>>a>>b;
        if(a==0&&b==0)
            break;
        if(a==b)
            printf("Case %d: Even.\n",i);
        else if(a==1||b==1)
            printf("Case %d: Odd.\n",i);
        else
            printf("Case %d: Even.\n",i);

        i++;
    }


}


#include<bits/stdc++.h>
using namespace std;

int main()
{
    int t;
    cin>>t;
    int co=0;
    while(t--)
    {
        int a,b,c,d;
        cin>>a>>b>>c>>d;
        if(a<=5)
        {
            if(b>5&&c>5&&d>5)
            {

                co=co+1;
                continue;
            }
        }

        if(b<=5)
        {
            if(a>5&&c>5&&d>5)
            {

                co=co+1;
                continue;
            }
        }
        if(c<=5)
        {
            if(b>5&&a>5&&d>5)
            {
                co=co+1;

                continue;
            }
        }
        if(d<=5)
        {
            if(b>5&&c>5&&a>5)
            {

                co=co+1;
                continue;
            }
        }
        if(a>5 && b>5 && c>5 && d>5)
        {
            co=co+1;


        }

    }
    cout<<co<<endl;


}

#include<bits/stdc++.h>
using namespace std;

int main()
{

int n,m;
cin>>n>>m;
int k=1;
for(int i=1;i<=n;i++)
{
    for(int j=1;j<=m;j++)
    {
        if(i%2==0)
           {
               if(k%2==0&&j==1)
                {cout<<"#";

                }
                else if(k%2!=0&&j==m)
                   {

                    cout<<"#";

                    }
                else
                    cout<<".";
                if(j==m)
                    k++;

           }

        else
            cout<<"#";
    }
    cout<<"\n";
}

}
